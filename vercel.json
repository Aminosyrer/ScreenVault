{
    "rewrites": [
        {
            "source": "/(.*)",
            "destination": "/"
        },
        {
            "source": "/login",
            "destination": "/login"
        },
        {
            "source": "/register",
            "destination": "/register"
        },
        {
            "source": "/movies/:id",
            "destination": "/movies/:id"
        },
        {
            "source": "/admin",
            "destination": "/admin"
        }
    ]
}